#include<stdio.h>
int main()
{
  	float width, length, Area; 
 
  	printf ("\n Please enter  Width of a Rectangle :  ");
  	scanf ("%f",&width);

  	printf ("\n Please enter the Length of a Rectangle :  ");
  	scanf ("%f",&length);
	   
  	Area = length * width;
 
	printf("\n The Area of a Rectangle =  %.2f", Area);
 
  	return 0;
}
