#include <stdio.h>

struct student {
    char name[50];
    int id;
    float gpa;
};

int main() {
    struct student s1;

    printf("enter student name: ");
    scanf("%s", s1.name);
    printf("Enter student ID: ");
    scanf("%d", &s1.id);
    printf("enter student GPA: ");
    scanf("%f", &s1.gpa);

    printf("Student name: %s\n", s1.name);
    printf("Student ID: %d\n", s1.id);
    printf("Student GPA: %f\n", s1.gpa);

    return 0;
}
